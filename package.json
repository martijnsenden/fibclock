{
	"name": "create-react-app-seed-js",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-scripts": "3.3.1"
	},
	"scripts": {
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"format": "npm run prettier -- --write",
		"lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx .",
		"start": "react-scripts start",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(css|html|js|json|jsx|md|mdx|scss)\"",
		"check-format": "npm run prettier -- --list-different",
		"test": "react-scripts test",
		"test:no-watch": "env CI=true react-scripts test",
		"validate": "npm-run-all --parallel check-format lint build test:no-watch"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint-config-prettier": "^6.10.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1"
	}
}
